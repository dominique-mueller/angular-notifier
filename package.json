		"@types/node": "12.11.x",
		"@types/web-animations-js": "2.2.x",
		"automatic-release": "1.1.x",
		"codecov": "3.6.x",
		"copyfiles": "2.1.x",
		"i": "^0.3.x",
		"jest": "^26.6.x",
