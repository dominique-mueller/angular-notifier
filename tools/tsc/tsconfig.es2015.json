{
	"extends": "./../../tsconfig.json",
	"baseUrl": "",
	"compilerOptions": {
		"declaration": true,
		"outDir": "./../../build/library-es2015",
		"rootDir": "./../../build/library-inline",
		"stripInternal": true,
		"target": "es2015",
		"types": [
			"@types/node"
		]
	},
	"files": [
		"./../../build/library-inline/index.ts",
		"./../../build/library-inline/typings.d.ts"
	],
	"angularCompilerOptions": {
		"annotateForClosureCompiler": true,
		"flatModuleId": "angular-notifier",
		"flatModuleOutFile": "angular-notifier.js",
		"genDir": "./../../build/library-es2015-gen-dir",
		"skipTemplateCodegen": true,
		"strictMetadataEmit": true
	}
}
